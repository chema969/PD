(define (letra_dni dni)
  (case (modulo dni 23)
    ((0) "T")
    ((1) "R")
    ((2) "W")
    ((3) "A")
    ((4) "G")
    ((5) "M")
    ((6) "Y")
    ((7) "F")
    ((8) "P")
    ((9) "D")
    ((10) "X")
    ((11) "B")
    ((12) "N")
    ((13) "J")
    ((14) "Z")
    ((15) "S")
    ((16) "Q")
    ((17) "V")
    ((18) "H")
    ((19) "L")
    ((20) "C")
    ((21) "K")
    ((22) "E")
    ))


(define (posicionCircunferenciaRecta r x0 y0 A B C)
(define (distanciaPuntoRecta x y a b c)
 (let(
      (sq (sqrt (+ (* a a) (* b b))))
      (recta (+ (* a x) (* b y) c))
     )
   (/ (abs recta) sq)))
  
  ;Calculo la distancia entre el centro y la recta

   (let(
      (d (distanciaPuntoRecta x0 y0 A B C))
     )
  (if (= r d )
    (display "tangente")
     (if (< r d)
     (display "exterior")
     (display "secante")
   
   )
  )
  )
)